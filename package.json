{
  "name": "dissys",
  "version": "0.0.0",
  "private": true,
  "main": "dist/bin/www",
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "npm run clean && tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/public/**/* dist/",
    "prestart": "npm run build",
    "start": "node ./dist/bin/www",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  }
}